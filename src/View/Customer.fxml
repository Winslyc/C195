<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="880.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.CustomerController">
   <children>
      <TableView fx:id="customerTable" layoutX="14.0" layoutY="2.0" prefHeight="349.0" prefWidth="847.0">
        <columns>
          <TableColumn id="idColumn" fx:id="idColumn" prefWidth="40.0" text="ID" />
          <TableColumn id="nameColumn" fx:id="nameColumn" prefWidth="82.0" text="Name" />
            <TableColumn id="addressColumn" fx:id="addressColumn" prefWidth="134.0" text="Address" />
            <TableColumn id="postalCodeColumn" fx:id="postalCodeColumn" prefWidth="71.0" text="Postal Code" />
            <TableColumn id="phoneColumn" fx:id="phoneColumn" prefWidth="86.0" text="Phone" />
            <TableColumn id="dateCreatedColumn" fx:id="dateCreatedColumn" prefWidth="89.0" text="Date Created" />
            <TableColumn id="createdByColumn" fx:id="createdByColumn" prefWidth="77.0" text="Created By" />
            <TableColumn id="lastUpdatedColumn" fx:id="lastUpdatedColumn" prefWidth="82.0" text="Last Update" />
            <TableColumn id="lastUpdatedByColumn" fx:id="lastUpdatedByColumn" prefWidth="110.0" text="Last Updated By" />
            <TableColumn id="divisionIdColumn" fx:id="divisionIdColumn" prefWidth="75.0" text="Division ID" />
        </columns>
      </TableView>
      <Button fx:id="addButton" layoutX="620.0" layoutY="361.0" mnemonicParsing="false" onAction="#onClickAddButton" text="Add" />
      <Button fx:id="deleteButon" layoutX="732.0" layoutY="361.0" mnemonicParsing="false" onAction="#onClickDeleteButton" text="Delete" />
      <Button fx:id="CancelButton" layoutX="798.0" layoutY="361.0" mnemonicParsing="false" onAction="#onClickCancel" text="Exit" />
      <Button fx:id="updateButton" layoutX="669.0" layoutY="361.0" mnemonicParsing="false" onAction="#updateCustomer" text="Update" />
   </children>
</AnchorPane>
